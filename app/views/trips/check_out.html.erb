<h1>Check-out Trip</h1>

<p>Status: <%= @trip.status %></p>

<%= form_with(model: @trip, local: true) do |form| %>
  <div class="field">
    <%= form.label :check_out_time %>
    <%= form.datetime_field :check_out_time %>
  </div>

  <%= form.submit 'Check-out' %>
<% end %>

<%= link_to 'Back', trips_path %>
