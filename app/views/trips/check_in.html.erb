<h1>Check-in Trip</h1>

<p>Status: <%= @trip.status %></p>

<%= form_with(model: @trip, local: true) do |form| %>
  <div class="field">
    <%= form.label :check_in_time %>
    <%= form.datetime_field :check_in_time %>
  </div>

  <%= form.submit 'Check-in' %>
<% end %>

<%= link_to 'Back', trips_path %>
